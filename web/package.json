        "jest": "22.4.2",
        "jquery": "3.3.1",
        "jsdom": "11.6.2",
        "nock": "9.2.3",
        "node-sass": "4.7.2",
        "npm-run-all": "4.1.2",
        "open": "0.0.5",
        "query-string": "6.0.0",
