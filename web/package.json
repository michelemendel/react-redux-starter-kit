{
    "name": "react-redux-starter-kit",
    "version": "1.0.0",
    "description": "Starter kit for React and Redux based on starter kit by Cory House",
    "scripts": {
        "start": "npm-run-all --parallel lint:watch devserver",
        "start:mocked": "npm-run-all --parallel lint:watch devserver:mocked",
        "devserver": "cross-env NODE_ENV=development webpack-dev-server",
        "devserver:mocked": "cross-env NODE_ENV=development MOCKED=mocked webpack-dev-server",
        "lint": "esw webpack.config.js src/**/*.ts  src/**/*.tsx",
        "lint:watch": "npm run lint -- --watch",
        "test": "npm run build:ts && jest",
        "test:watch": "npm run build:ts && jest --watch",
        "test:coverage": "jest --coverage",
        "clean": "rimraf target/dist && rimraf target/ts_build",
        "build:prod:skiptests": "npm run clean && cross-env NODE_ENV=production webpack -p",
        "build:dev:skiptests": "npm run clean && cross-env NODE_ENV=development webpack --display-error-details",
        "build:prod": "npm-run-all lint build:prod:skiptests test",
        "build:dev": "npm-run-all lint build:dev:skiptests test",
        "build": "npm run build:dev:skiptests",
        "build:ts": "tsc -m es6 --outDir target/ts_build"
    },
    "author": "Michele Mendel",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": ""
    },
    "devDependencies": {
        "@types/jest": "22.2.0",
        "@types/react": "16.0.40",
        "@types/react-dom": "16.0.4",
        "@types/react-fontawesome": "1.6.2",
        "babel-core": "6.26.0",
        "babel-jest": "^23.0.0-alpha.0",
        "babel-loader": "7.1.4",
        "babel-plugin-react-display-name": "2.0.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-env": "1.6.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-react-hmre": "1.1.1",
        "babel-register": "6.26.0",
        "bootstrap": "4.0.0",
        "chai": "4.1.2",
        "colors": "1.2.1",
        "compression": "1.7.2",
        "cross-env": "5.1.4",
        "css-loader": "0.28.11",
        "css-reset-and-normalize": "0.3.0",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
        "eslint": "4.19.0",
        "eslint-loader": "2.0.0",
        "eslint-plugin-import": "2.9.0",
        "eslint-plugin-json": "1.2.0",
        "eslint-plugin-react": "7.7.0",
        "eslint-plugin-typescript": "0.10.0",
        "eslint-watch": "3.1.3",
        "eventsource-polyfill": "0.9.6",
